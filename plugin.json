{
  "id": "nodebb-plugin-sso-discord",
  "name": "NodeBB Discord SSO",
  "description": "NodeBB Plugin that allows users to login/register via Discord",
  "url": "https://github.com/julianlam/nodebb-plugin-sso-oauth",
  "library": "./library.js",
  "templates": "templates",
  "hooks": [
    { "hook": "static:app.load", "method": "init" },
    { "hook": "filter:user.delete", "method": "deleteUserData" },
    { "hook": "filter:auth.init", "method": "getStrategy" },
    { "hook": "filter:admin.header.build", "method": "addMenuItem" }
  ],
  "acpScripts": [
    "acp/admin/admin.js"
  ]
}
